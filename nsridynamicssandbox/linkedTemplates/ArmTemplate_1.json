							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"dataflow": {
								"referenceName": "MG DO Monthend Aggregation",
								"type": "DataFlowReference",
								"parameters": {
									"dateconverted": {
										"value": "'@{pipeline().parameters.dateconverted}'",
										"type": "Expression"
									}
								},
								"datasetParameters": {
									"donororder": {},
									"bankdetails": {},
									"accounttransactions": {},
									"ContactTransactions": {},
									"DonorOrderTrxDate": {}
								}
							},
							"staging": {},
							"compute": {
								"coreCount": 8,
								"computeType": "General"
							},
							"traceLevel": "Fine"
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {},
					"cancelAfter": {}
				},
				"parameters": {
					"dateconverted": {
						"type": "string",
						"defaultValue": "2022-12-05"
					}
				},
				"annotations": []
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/Tst - MG DO Month-end Transactions')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "MG DO Monthend Transactions",
						"type": "ExecuteDataFlow",
						"dependsOn": [],
