{
	"name": "Update Donor Order from CSV File",
	"properties": {
		"activities": [
			{
				"name": "updatedonororderfromfile",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFileName": "*",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "CommonDataServiceForAppsSink",
						"writeBatchSize": 10,
						"writeBehavior": "upsert"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "cog_donororderid",
									"type": "String"
								},
								"sink": {
									"name": "cog_donororderid",
									"type": "Guid"
								}
							},
							{
								"source": {
									"name": "cog_transactionid",
									"type": "String"
								},
								"sink": {
									"name": "cog_donortransaction",
									"type": "Guid"
								}
							}
						]
					}
				},
				"outputs": [
					{
						"referenceName": "SandboxDonorOrder",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}